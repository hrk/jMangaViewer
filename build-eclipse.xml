<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project jMangaViewer with Jar-in-Jar Loader">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="${basedir}/jMangaViewer.jar">
            <manifest>
                <attribute name="Main-Class" value="org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader"/>
                <attribute name="Rsrc-Main-Class" value="it.sineo.jMangaViewer.Launcher"/>
                <attribute name="Class-Path" value="."/>
                <attribute name="Rsrc-Class-Path" value="./ commons-logging-1.1.1.jar tika-app-1.4.jar common-image-3.10.1.jar common-io-3.10.1.jar common-lang-3.10.1.jar imageio-core-3.10.1.jar imageio-jpeg-3.10.1.jar imageio-metadata-3.10.1.jar imgscalr-lib-4.2.jar"/>
            </manifest>
            <zipfileset src="jar-in-jar-loader.zip"/>
            <fileset dir="${basedir}/bin"/>
            <fileset dir="/Users/luca/Documents/programmazione/git/java-unrar/target"/>
            <zipfileset dir="/Users/luca/Documents/programmazione/java/lib/commons-logging-1.1.1" includes="commons-logging-1.1.1.jar"/>
            <zipfileset dir="/Users/luca/Documents/programmazione/java/lib/apache-tika-1.4" includes="tika-app-1.4.jar"/>
            <zipfileset dir="${basedir}/libs" includes="common-image-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="common-io-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="common-lang-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="imageio-core-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="imageio-jpeg-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="imageio-metadata-3.10.1.jar"/>
            <zipfileset dir="${basedir}/libs" includes="imgscalr-lib-4.2.jar"/>
        </jar>
    </target>
</project>
